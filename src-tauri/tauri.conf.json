{
  "productName": "selenite-geos-graph-editor",
  "version": "0.0.1",
  "identifier": "fr.selenite.geos-graph-editor",
  "build": {
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm run build",
    "frontendDist": "../build"
  },
  "app": {
    "windows": [
      {
        "title": "Selenite - GEOS Graph Editor",
        "width": 800,
        "height": 600,
        "center": true,
        "resizable": true,
        "maximized": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
